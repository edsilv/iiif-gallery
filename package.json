{
  "name": "iiif-gallery",
  "version": "0.0.23",
  "description": "Display any set of IIIF objects that have thumbnails as a gallery",
  "module": "dist/esm/iiifgallery.js",
  "main": "dist/iiifgallery.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "browser": "dist/iiifgallery.js",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@stencil/core": "~0.13.2",
    "manifesto.js": "3.0.6"
  },
  "devDependencies": {
    "@types/jest": "^21.1.1",
    "classnames": "^2.2.5",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edsilv/iiif-gallery.git"
  },
  "author": "@edsilv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edsilv/iiif-gallery"
  },
  "homepage": "https://github.com/edsilv/iiif-gallery",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "keywords": [
    "iiif",
    "gallery",
    "webcomponent"
  ]
}
