{
  "hosting": {
    "rules": [
      {
        "include": "/",
        "headers": [
          {
            "name": "Link",
            "value": "</build/iiifgallery/iiifgallery.core.js>;rel=preload;as=script"
          }
        ]
      },
      {
        "include": "/build/iiifgallery/**",
        "headers": [
          {
            "name": "Cache-Control",
            "value": "public, max-age=31536000"
          }
        ]
      }
    ]
  }
}